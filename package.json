{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^2.14.0",
    "gatsby-image": "^2.2.14",
    "gatsby-plugin-manifest": "^2.2.9",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-sass": "^2.1.12",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-json": "^2.2.6",
    "gatsby-transformer-sharp": "^2.2.12",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-feather": "^1.1.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "breakpoint-sass": "^2.7.1",
    "chroma-sass": "^1.2.6",
    "conventional-changelog-cli": "^2.0.23",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby-plugin-typescript": "^2.1.5",
    "husky": "^2.7.0",
    "js-logger": "^1.6.0",
    "lint-staged": "^8.2.1",
    "normalize-scss": "^7.0.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "typescript": "^3.5.3",
    "typey": "^1.1.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint --ignore-path .gitignore ./src --ext ts --ext tsx --ext js --ext jsx",
    "format": "eslint --ignore-path .gitignore ./src --ext ts --ext tsx --ext js --ext jsx --fix; tsc",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "type-check": "tsc",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
